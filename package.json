    "next": "9.1.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-draft-wysiwyg": "^1.14.4",
    "react-hook-form": "^4.1.0",
    "react-images": "^1.0.0",
    "react-photo-gallery": "^8.0.0",
    "react-player": "^1.14.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
